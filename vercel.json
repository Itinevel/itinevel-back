{
    "version": 2,
    "builds": [
      { "src": "./api/index.ts",
        "use": "@vercel/node"
       }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/api", "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
    ] }
    ],
    "buildCommand": "npm run build"
  }
  
  